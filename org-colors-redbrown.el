(custom-set-variables
 ;;; This sets some nice reddish brown colors for the org headers
 ;;; Only problem is that there is not much contrast between adjacent headers
 '(org-date ((((class color) (background light)) (:foreground "DarkSeaGreen4" :underline t))))
 '(org-done ((t (:foreground "chartreuse3" :overline "gray" :underline "gray" :weight bold :height 0.85))))
 '(org-level-1 ((((class color) (min-colors 88) (background light)) (:foreground "#501005"))))
 '(org-level-2 ((((class color) (min-colors 16) (background light)) (:foreground "IndianRed4"))))
 '(org-level-3 ((((class color) (min-colors 88) (background light)) (:foreground "Firebrick3"))))
 '(org-level-4 ((((class color) (min-colors 88) (background light)) (:foreground "IndianRed3"))))
 '(org-level-5 ((((class color) (min-colors 16) (background light)) (:foreground "Firebrick"))))
 '(org-level-6 ((((class color) (min-colors 16) (background light)) (:foreground "IndianRed"))))
 '(org-level-7 ((((class color) (min-colors 16) (background light)) (:foreground "Firebrick2"))))
 '(org-level-8 ((((class color) (min-colors 16) (background light)) (:foreground "IndianRed2"))))
 '(org-link ((((class color) (background light)) (:foreground "MidnightBlue" :underline t))))
 '(org-table ((((class color) (min-colors 88) (background light)) (:foreground "Blue4"))))
 '(org-time-grid ((((class color) (min-colors 16) (background light)) (:foreground "DarkGoldenrod"))))
 '(org-todo ((t (:foreground "Red2" :overline "gray" :underline "gray" :weight bold :height 0.85))))
)
